{% extends "Partie_admin/Shared/base.html.twig" %}

{% block title %}Marché des Plans  Administration --Voir les informations de la Forme
{% endblock %}

{% block body %}
	<h1>Forme</h1>

	<table class="table table-striped projects">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ forme.id }}</td>
			</tr>
			<tr>
				<th>Libelle</th>
				<td>{{ forme.libelle }}</td>
			</tr>
		</tbody>
	</table>
	<div class="row">
		<div class="col-md-2">
			<a href="{{ path('forme_index') }}">Revenir à la liste</a>
		</div>
		<div class="col-md-2">
			<a href="{{ path('forme_edit', {'id': forme.id}) }}">Modifier</a>
		</div>
		<div class="col-md-2">
			{{ include('forme/_delete_form.html.twig') }}
		</div>
	</div>

{% endblock %}
